<%- include("template/cabecera", {tituloWeb: 'Nuevo usuario' }) %>
    <div class="container">
        <h1>Crear Nuevo usuario</h1>
        <form action="/usuario" class="my-2" method="POST">
            <input type="text" placeholder="Nombre " class="form-control my-2" name="nombre">
            <input type="text" placeholder="Usuario" class="form-control my-2" name="usuario">
            <input type="text" placeholder="Contraseña" class="form-control my-2" name="contraseña">
            <input type="text" placeholder="Direccion_de_envio" class="form-control my-2" name="Direccion_de_envio">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
<%- include("template/footer") %>
